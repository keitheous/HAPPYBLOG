<h1> Displaying all Posts: </h1>
</form><form action="/posts/new">
<button> New Post </button>
</form>
<br>
<% @posts.each do |post| %>
  <p><a href="/posts/<%= post.id %>"><b> Post Title : <%= post.title %></b></a></p>
   <% @users.each do |user| %>
     <% if user.id == post.user_id%>
     <p> Name: <%= user.name %></p>
     <% end %>
   <% end %>
  <p> Post: </p>
  <p> <%= post.body %></p>
  <br>
<% end %>
